<div class="controls_popupTemplate_theme-{{this._options.theme}} controls-Popup ws-float-area-show-complete"
     on:animationend="_animated()"
     on:transitionend="_animated()"
     on:keyup="_keyUp()"
     on:keydown="_keyDown()"
     templateName="{{ _compatibleTemplateName}}"
     template="{{ _stringTemplate ? _options.template}}">
    <ws:partial
            attr:class="controls-Popup__template {{_options.position.height ? 'controls-Popup__template-height'}}"
            template="{{_options.content}}"
            _prefetchData="{{_prefetchData || _options.prefetchData}}"
            _isPrefetchDataMode="{{_isPrefetchDataMode}}"
            scope="{{_options}}"
            opener="{{null}}"
            __key="{{_options.id}}_content"
            name="{{_options.id}}_content"
            on:sendResult="_sendResult()"
            on:close="_close()"
            on:popupDragEnd="_popupDragEnd()"
            on:popupDragStart="_popupDragStart()"
            on:popupResizingLine="_popupResizingLine()"
            on:popupMouseEnter="_popupMouseEnter()"
            on:popupMouseLeave="_popupMouseLeave()"
            on:_documentDragStart="_documentDragHandler('Start')"
            on:documentDragStart="_documentDragHandler('Start')"
            on:_documentDragEnd="_documentDragHandler('End')"
            on:documentDragEnd="_documentDragHandler('End')"
            on:maximized="_maximized()"
            on:register="_registerHandler()"
            on:unregister="_unregisterHandler()"
            on:controlResize="_controlResizeHandler()"
            on:showIndicator="_showIndicatorHandler()"
            on:registerPending="_registerPendingHandler()"
            on:cancelFinishingPending="_cancelFinishingPendingHandler()"
            on:finishPendingOperations="_finishPendingOperationsHandler()" />
</div>

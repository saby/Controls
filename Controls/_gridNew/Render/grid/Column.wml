<ws:if data="{{ !!contentTemplate }}">
   <ws:partial
           template="{{ contentTemplate }}"
           column="{{ column }}"
           itemData="{{ (item || itemData) }}"
           item="{{ (item || itemData) }}"
           itemActionsTemplate="{{ itemActionsTemplate }}"/>
</ws:if>
<ws:else>
   {{ (item || itemData).getContents()[column.displayProperty] }}
</ws:else>

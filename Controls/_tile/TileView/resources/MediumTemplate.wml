<ws:partial template="wml!Controls/_tile/TileView/TileTpl"
            height="160"
            highlightOnHover="{{false}}"
            itemActionsClass="controls-TileView__itemActions_bottomRight controls-TileView__mediumTemplate_itemActions"
            staticHeight="{{true}}"
            scope="{{_options}}">
   <ws:contentTemplate>
      <div class="controls-TileView__mediumTemplate_content">
         <ws:partial template="wml!Controls/_tile/TileView/TileImageTemplate"
                     attr:class="controls-TileView__mediumTemplate_image controls-TileView__mediumTemplate_image"/>
         <span class="controls-TileView__mediumTemplate_title
                      controls-fontsize-l
                      controls-text-secondary
                      {{titleLines > 1 ? 'controls-TileView__text_ellipsis_multiLine' : 'ws-ellipsis'}}
                      controls-fontweight-bold
                      controls-TileView__mediumTemplate_title"
               style="-webkit-line-clamp: {{titleLines || 1}};"
               title="{{itemData.item[itemData.displayProperty]}}">
            {{itemData.item[itemData.displayProperty]}}
         </span>
      </div>
   </ws:contentTemplate>
</ws:partial>

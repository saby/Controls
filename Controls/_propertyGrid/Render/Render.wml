<div class="controls-PropertyGrid controls-PropertyGrid_theme-{{_options.theme}}">
    <ws:partial template="wml!Controls/_propertyGrid/Render/resources/for"
                itemsViewModel="{{_options.listModel}}"
                theme="{{_options.theme}}"
                itemTemplate="{{_options.itemTemplate}}"
                groupTemplate="{{_options.groupTemplate || _groupTemplate}}"

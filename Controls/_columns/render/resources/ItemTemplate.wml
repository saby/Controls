<div attr:class="{{ item.getWrapperClasses(highlightOnHover, theme, cursor || clickable) }}">
   <ws:if data="{{item.shouldDisplayMarker(marker)}}">
      <div class="controls-ColumnsView__item_marker"></div>
   </ws:if>
   <ws:if data="{{ multiSelectVisibility !== 'hidden' }}">
      <ws:partial template="{{ multiSelectTpl }}" attr:class="{{ item.getMultiSelectClasses(theme) }}">
         <ws:templateOptions item="{{ item }}"/>
      </ws:partial>
   </ws:if>
   <ws:if data="{{ contentTemplate }}">
      <div attr:class="{{ item.getContentClasses(theme) }}">
         <ws:partial
            template="{{ contentTemplate }}"
            item="{{ item }}"
            useNewModel="{{ true }}"
            searchValue="{{ searchValue }}"
            itemActionsTemplate="{{ itemActionsTemplate }}"
            swipeTemplate="{{ swipeTemplate }}" />
      </div>
   </ws:if>
   <ws:else>
      <div attr:class="{{ item.getContentClasses(theme) }}">
         {{ item.getContents()[item.getDisplayProperty() || 'title'] }}
      </div>
   </ws:else>
   <ws:if data="{{ item.hasVisibleActions() || item.isEditing() }}">
      <ws:if data="{{ item.isSwiped() }}">
         <ws:partial
            template="{{ swipeTemplate }}"
            item="{{ item }}"
            itemActionsTemplate="{{ itemActionsTemplate }}">
         </ws:partial>
      </ws:if>
      <ws:else data="{{ itemActionsPosition !== 'custom' }}">
         <ws:partial
            template="{{ itemActionsTemplate }}"
            itemActionsClass="{{ itemActionsClass }}"
            item="{{ item }}" />
      </ws:else>
   </ws:if>
</div>

<div class="controls-FilterSelect">
   <ws:for data="index, item in _options.items">
            <span if="{{index}}"> / </span>
      <ws:partial
         template="{{_options.itemTemplate || _itemTemplate}}"
         displayProperty="{{_options.displayProperty}}"
         index="{{index}}"
         item="{{item}}"
         on:click="_clickHandler(item)"/>
   </ws:for>
</div>

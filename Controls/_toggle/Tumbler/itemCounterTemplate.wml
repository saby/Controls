<ws:partial template="wml!Controls/_toggle/Tumbler/itemTemplate" item="{{item}}">
    <ws:contentTemplate>
        <ws:partial template="{{contentTemplate.captionTemplate}}"/>
        <ws:if data="{{item.get(counterProperty || 'counter')}}">
            <span class="controls-Tumbler__button-counter">{{item.get(counterProperty || 'counter')}}</span>
        </ws:if>
    </ws:contentTemplate>
</ws:partial>

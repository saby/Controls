<ws:if data="{{ __viewConfig && __viewConfig.mode != 'dialog' && __viewConfig.template }}">
   <ws:partial
      template="{{ __viewConfig.template }}"
      scope="{{ __viewConfig.options || {} }}"
   />
</ws:if>
<ws:else>
    <ws:partial template="{{ _options.content }}" scope="{{ _options }}"/>
</ws:else>

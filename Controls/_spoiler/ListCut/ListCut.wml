<div class="controls-ListCut ws-flexbox ws-flex-column">
   <ws:partial template="{{_options.content}}" navigation="{{ _expanded ? _options.collapsedNavigation : _options.expandedNavigation }}"/>
   <Controls.toggle:BigSeparator 
         value="{{_expanded}}"
         iconSize="{{_options.iconSize}}"
         readOnly="{{_options.readOnly}}"
         on:valueChanged="_clickHandler()"/>
</div>

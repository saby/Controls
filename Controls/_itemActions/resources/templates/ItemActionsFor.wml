<ws:for data="action in showedActions">
    <ws:partial
            template="wml!Controls/_itemActions/resources/templates/ItemAction"
            itemData="{{itemData}}"
            actionStyle="{{actionStyle}}"
            actionMode="{{actionMode}}"
            iconStyle="{{iconStyle}}"
            size="{{size}}"
            action="{{action}}"/>
</ws:for>

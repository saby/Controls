<colgroup class="{{ colgroup.getBodyClasses() }}">
    <ws:for data="colgroupCell in colgroup.getCells()">
        <col attr:key="{{ colgroupCell.getKey() }}"
             class="{{ colgroupCell.getBodyClasses(theme) }}"
             style="{{ colgroupCell.getBodyStyles() }}"/>
    </ws:for>
</colgroup>

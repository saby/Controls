<span attr:class="
            controls-Decorator_theme-{{_options.theme}}">
   <ws:for data="path in _parsedText">
      <ws:if data="{{path.type === 'highlight'}}">
         <span class="{{_className}}">{{path.value}}</span>
      </ws:if>
      <ws:else>{{path.value}}</ws:else>
   </ws:for>
</span>

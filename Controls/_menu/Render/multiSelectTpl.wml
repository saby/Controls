<Controls.Container.Async templateName="Controls/toggle:Checkbox"
                          on:valueChanged="_proxyEvent('checkBoxClick')"
                          attr:class="controls-Menu__row-checkbox
                                      controls-DropdownList__row-checkbox
                                      controls-Menu__row-checkbox-{{checkBoxAlign === 'right' ? 'right' : 'left'}}_theme-{{_options.theme}}
                                      controls-Menu__row-checkbox_{{itemData.isEmptyItem ? 'invisible' :
                                      treeItem.isSelected() ? 'active' : 'default'}}_theme-{{_options.theme}}">
   <ws:templateOptions value="{{treeItem.isSelected()}}"/>
</Controls.Container.Async>

<Controls.Container.Async templateName="Controls/toggle:Checkbox">
   <ws:partial template="{{content.resolvedTemplate}}"
               attr:class="js-controls-ListView__notEditable controls-DragNDrop__notDraggable js-controls-ListView__checkbox"
               triState="{{triState}}"
               readOnly="{{checkboxReadOnly || false}}"
               value="{{multiSelectStatus}}"
               on:valueChanged="_onCheckBoxClick(key, multiSelectStatus)"
   />
</Controls.Container.Async>
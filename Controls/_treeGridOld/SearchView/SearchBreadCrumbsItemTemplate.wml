<ws:partial template="{{itemData.resolveItemTemplate(itemData)}}"
            scope="{{_options}}"
            colspan="{{itemData.getColspan(colspan, itemData.columnScrollVisibility)}}"
            colspanLength="{{ itemData.getColspanLength(colspan, itemData.columnScrollVisibility) }}"
            renderSeparatedColumnContent="{{ colspan === false }}"
            expanderIcon="none"
            marker="{{ false }}"
            attr:class="controls-TreeGrid__row__searchBreadCrumbs js-controls-ListView__notEditable
            {{ itemData.columnScroll && !itemData.columnScrollVisibility ? 'controls-TreeGrid__row__searchBreadCrumbs_colspaned' }}
            {{ itemData.shouldHideColumnSeparator(colspan, itemData.columnScrollVisibility) ? 'controls-Grid__row__searchBreadCrumbs_withoutColumnSeparator' }}"
            attr:key="{{ itemData.key }}__breadcrumbs">
    <ws:colspanTemplate>
        <Controls.Container.Async templateName="Controls/breadcrumbs:Path">
            <ws:partial template="{{ content.resolvedTemplate }}"
                        items="{{colspanTemplate.itemData.dispItem.getBreadcrumbs()}}"
                        backgroundStyle="{{_options.backgroundStyle}}"
                        attr:class="controls-Grid__breadCrumbs {{ itemData.columnScroll && !itemData.columnScrollVisibility ? 'controls-TreeGrid__breadCrumbs_colspaned' }}"
                        breadCrumbsItemClickCallback="{{ itemData.breadcrumbsItemClickCallback }}"
                        keyProperty="{{itemData.keyProperty}}"
                        displayProperty="{{itemData.breadCrumbsDisplayProperty}}">
                <ws:itemTemplate>
                    <ws:partial template="{{itemData.searchBreadCrumbsItemContent}}">
                        <ws:contentTemplate>
                            <ws:partial template="wml!Controls/_treeGridOld/SearchView/SearchBreadCrumbsContent" searchValue="{{itemData.searchValue}}"/>
                        </ws:contentTemplate>
                    </ws:partial>
                </ws:itemTemplate>
            </ws:partial>
        </Controls.Container.Async>
    </ws:colspanTemplate>
</ws:partial>

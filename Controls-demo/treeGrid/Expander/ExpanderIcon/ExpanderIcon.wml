<div class="controlsDemo__wrapper controlsDemo__flexRow">

    <div class="controlsDemo__cell controlsDemo__mr2 controlsDemo_fixedWidth300">
        <Router.router:Reference attr:class="controls-text-label"
                                 state="/Controls-demo/app/:app"
                                 app="{{ 'Controls-demo/treeGrid/Expander/ExpanderIcon/None/Index' }}">
            <a href="{{content.href}}">Экспандер не показывается</a>
        </Router.router:Reference>
        <Controls-demo.treeGrid.Expander.ExpanderIcon.None.Index/>
    </div>

    <div class="controlsDemo__cell controlsDemo__mr2 controlsDemo_fixedWidth300">

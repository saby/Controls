<div class="controlsDemo__flexColumn">
    <div class="controlsDemo__wrapper controlsDemo__flex controlsDemo__flex-jc-sa" style="align-items: flex-start">
        <div class="controlsDemo__cell">
            <div class="controls-text-label_theme-{{_options.theme}} controlsDemo__maxWidth200">readOnly=false</div>
            <Controls.dateRange:RangeShortSelector
                    bind:startValue="_startValue1"
                    bind:endValue="_endValue1"
                    chooseHalfyears="{{false}}"
                    chooseMonths="{{false}}"
                    readOnly="{{false}}"
                    chooseQuarters="{{false}}"/>
        </div>
        <div class="controlsDemo__cell">
            <div class="controls-text-label_theme-{{_options.theme}} controlsDemo__maxWidth200">readOnly=true</div>
            <Controls.dateRange:RangeShortSelector
                    bind:startValue="_startValue1"
                    bind:endValue="_endValue1"
                    chooseHalfyears="{{false}}"
                    readOnly="{{true}}"
                    chooseMonths="{{false}}"/>
        </div>
    </div>
</div>

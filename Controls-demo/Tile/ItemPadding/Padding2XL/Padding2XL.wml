<div class="controlsDemo__wrapper">
    <div class="controlsDemo__cell controlsDemo__mr2 controlsDemo-hor-padding-2xl">
        <Controls.tile:View
                tileMode="dynamic"
                displayProperty="title"
                imageProperty="image"
                keyProperty="id"
                source="{{_viewSource}}">
            <ws:itemPadding left="2xl" right="2xl"/>
            <ws:itemTemplate>
                <ws:partial
                    template="Controls/tile:PreviewTemplate"
                    hasTitle="{{false}}"
                    scope="{{itemTemplate}}"/>
            </ws:itemTemplate>
        </Controls.tile:View>
    </div>
</div>

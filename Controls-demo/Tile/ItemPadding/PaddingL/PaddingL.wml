<div class="controlsDemo__wrapper">
    <div class="controlsDemo__cell controlsDemo__mr2 controlsDemo-hor-padding-l">
        <Controls.tile:View
                tileMode="dynamic"
                displayProperty="title"
                imageProperty="image"
                keyProperty="id"
                source="{{_viewSource}}">
            <ws:itemPadding left="l" right="l" bottom="l" top="l"/>
            <ws:itemTemplate>
                <ws:partial
                        template="Controls/tile:PreviewTemplate"
                        hasTitle="{{false}}"
                        scope="{{itemTemplate}}"/>
            </ws:itemTemplate>
        </Controls.tile:View>
    </div>
</div>

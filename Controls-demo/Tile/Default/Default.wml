<div class="controlsDemo__wrapper controlDemo_Tile_default">
<Controls.tile:View
        tileMode="dynamic"
        displayProperty="title"
        imageProperty="image"
        keyProperty="id"
        source="{{_viewSource}}">
    <ws:itemTemplate>
        <ws:partial
                template="Controls/tile:PreviewTemplate"
                hasTitle="{{false}}"
                scope="{{itemTemplate}}"/>
    </ws:itemTemplate>
</Controls.tile:View>
</div>

<ws:partial template="Controls/gridNew:ColumnTemplate" itemData="{{itemData}}">
<ws:contentTemplate>
    <ws:partial template="Controls/list:EditingTemplate"
                value="{{ itemData.item[column.config.displayProperty || 'title'] }}"
                itemData="{{itemData}}"
                enabled="{{true}}">
        <ws:editorTemplate>
            <Controls.input:Text bind:value="itemData.item[column.config.displayProperty || 'title']"
                                 selectOnClick="{{ false }}" />
        </ws:editorTemplate>
    </ws:partial>
</ws:contentTemplate>
</ws:partial>

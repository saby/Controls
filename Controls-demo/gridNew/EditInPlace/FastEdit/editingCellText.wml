<ws:partial template="Controls/gridNew:ColumnTemplate" itemData="{{gridColumn}}">
    <ws:contentTemplate>
        <ws:partial template="Controls/list:EditingTemplate"
                    value="{{ itemData.item[column.config.displayProperty] }}"
                    itemData="{{itemData}}"
                    enabled="{{true}}">
            <ws:editorTemplate>
                <Controls.input:Text
 contrastBackground="{{true}}"                        attr:class="js-controls-Grid__editInPlace__input-1"
                        bind:value="itemData.item[column.config.displayProperty]"
                        selectOnClick="{{ false }}" />
            </ws:editorTemplate>
        </ws:partial>
    </ws:contentTemplate>
</ws:partial>

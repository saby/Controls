<ws:partial template="Controls/grid:ColumnTemplate" itemData="{{gridColumn}}">
<ws:contentTemplate>
    <ws:partial template="Controls/list:EditingTemplate"
                value="{{ item.contents[column.config.displayProperty || 'title'] }}"
                scope="{{ _options }}"
                enabled="{{true}}">
        <ws:editorTemplate>
            <Controls.validate:InputContainer name="InputValidate">
                <ws:validators>
                    <ws:Function value="{{item.contents[column.config.displayProperty || 'title']}}">Controls-demo/gridNew/EditInPlace/Validation/Index:LengthChecker</ws:Function>
                </ws:validators>
                <ws:content>
                    <Controls.input:Text contrastBackground="{{true}}" bind:value="item.contents[column.config.displayProperty || 'title']"
                                         selectOnClick="{{ false }}" />
                </ws:content>
            </Controls.validate:InputContainer>
        </ws:editorTemplate>
    </ws:partial>
</ws:contentTemplate>
</ws:partial>

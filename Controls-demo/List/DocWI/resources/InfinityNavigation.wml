<Controls.scroll:Container attr:style="height: 220px">
    <Controls.list:View
            source="{{_viewSource}}"
            keyProperty="id">
        <ws:navigation source="page" view="infinity">
            <ws:sourceConfig pageSize="{{9}}" page="{{0}}" hasMore="{{false}}"></ws:sourceConfig>
            <ws:viewConfig pagingMode="basic"></ws:viewConfig>
        </ws:navigation>
    </Controls.list:View>
</Controls.scroll:Container>

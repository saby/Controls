<ws:partial template="Controls/gridOld:HeaderContent" colData="{{colData}}" attr:class="controlsDemo__Grid_header-gridTitle__cell">
    <ws:contentTemplate>
        <div class="controlsDemo__Grid_header-gridTitle-spacing">
            <Controls.heading:Title caption="{{colData.column.captionForGrid}}" fontColorStyle="primary" fontSize="l" readOnly="{{true}}"/>
            <Controls.buttons:Button viewMode="toolButton" icon="icon-Add" iconStyle="secondary" fontSize="m" inlineHeight="default" iconSize="s"
                                     attr:class="controlsDemo__Grid_header-gridTitle_addButton"/>
        </div>
    </ws:contentTemplate>
</ws:partial>

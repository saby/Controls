<div class="controlsDemo__wrapper controlsDemo_fixedWidth500 controlsDemo__itemActions_list_delayed">
    <Controls.list:View
            keyProperty="id"
            source="{{_viewSource}}"
            itemActions="{{_itemActions}}"
            itemActionsVisibility="{{_itemActionsVisibility}}">
        <ws:itemTemplate>
            <ws:partial template="Controls/list:ItemTemplate">
                <ws:contentTemplate>
                    <div style="padding: 3px;">
                        <div>{{itemTemplate.itemData.item.title}}</div>
                        <div style="font-size: 11px; color: grey">{{itemTemplate.itemData.item.description}}</div>
                    </div>
                </ws:contentTemplate>
            </ws:partial>
        </ws:itemTemplate>
    </Controls.list:View>
</div>

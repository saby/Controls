<div class="controlsDemo__wrapper controlsDemo__flexRow">
    <div class="controlsDemo__wrapper__horizontal controlsDemo__cell controlsDemo__ml1">
         <div class="controls-text-label">Отображение в поле ввода имени руководителя отдела</div>
         <Controls.suggest:Selector
                 attr:class="controlsDemo__input  controlsDemo__ml1 "
                 bind:value="_suggestDownValue"
                 displayProperty="owner"
                 searchParam="title"
                 placeholder="Введите название отдела"
                 navigation="{{_navigation}}"
                 source="{{_source}}">
             <ws:suggestTemplate templateName="wml!Controls-demo/Suggest_new/Selector/DisplayProperty/resources/SuggestTemplate">
                 <ws:templateOptions demoClass="demo-SuggestInputList"/>
             </ws:suggestTemplate>
         </Controls.suggest:Selector>
    </div>
</div>

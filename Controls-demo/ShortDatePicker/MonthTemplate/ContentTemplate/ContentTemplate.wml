<Controls.dateRange:LiteSelector
        source="{{_source}}"
        attr:test_name="default_range">
    <ws:monthTemplate>
        <ws:partial periods="{{_displayedRanges2}}" template="Controls/shortDatePicker:MonthTemplate">
            <ws:contentTemplate>
                <div>
                    {{_formatMonth(contentTemplate.value)}}
                </div>
            </ws:contentTemplate>
        </ws:partial>
    </ws:monthTemplate>
</Controls.dateRange:LiteSelector>

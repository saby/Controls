<div class="controlsDemo__wrapper">
   <div class="controlsDemo__cell">
      <div class="controls-text-label_theme-{{_options.theme}}">Не определен precision</div>
      <Controls.decorator:Number value="{{_value}}"/>
   </div>
   <div class="controlsDemo__cell">
      <div class="controls-text-label_theme-{{_options.theme}}">precision = 0</div>
      <Controls.decorator:Number value="{{_value}}" precision="{{0}}"/>
   </div>
   <div class="controlsDemo__cell">
      <div class="controls-text-label_theme-{{_options.theme}}">precision = 3</div>
      <Controls.decorator:Number value="{{_value}}" precision="{{3}}"/>
   </div>
   <div class="controlsDemo__cell">
      <div class="controls-text-label_theme-{{_options.theme}}">precision = 10</div>
      <Controls.decorator:Number value="{{_value}}" precision="{{10}}"/>
   </div>
</div>

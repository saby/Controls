<div class="controlsDemo__wrapper controlsDemo_fixedWidth500">
    <div class="controlsDemo__cell">
        <Controls.toggle:Chips items="{{_items1}}" bind:selectedKeys="_selectedKeys1">
            <ws:itemTemplate>
                <ws:partial template="Controls/toggle:chipsItemCounterTemplate" scope="{{itemTemplate}}"/>
            </ws:itemTemplate>
        </Controls.toggle:Chips>
    </div>
    <div class="controlsDemo__cell">
        <Controls.toggle:Chips items="{{_items1}}" bind:selectedKeys="_selectedKeys1">
            <ws:itemTemplate>
                <ws:partial template="Controls/toggle:chipsItemCounterTemplate"
                            counterStyle="{{ itemTemplate.item.get('color') }}"
                            scope="{{itemTemplate}}"/>
            </ws:itemTemplate>
        </Controls.toggle:Chips>
    </div>
</div>

        <div class="controls-text-label">2 кнопки</div>
        <Controls.toggle:Chips items="{{_items1}}" bind:selectedKeys="_selectedKeys1" attr:class="ControlsDemo-ButtonGroup__twoButtons"/>
    </div>
    <div class="controlsDemo__cell controlsDemo_fixedWidth500">
        <div class="controls-text-label">Несколько кнопок</div>

    </div>
    <div class="controlsDemo__cell controlsDemo_fixedWidth500">
        <div class="controls-text-label">С иконками</div>
        <Controls.toggle:Chips items="{{_items3}}" bind:selectedKeys="_selectedKeys3" attr:class="ControlsDemo-ButtonGroup__muiltyIconButtons"/>
    </div>
</div>

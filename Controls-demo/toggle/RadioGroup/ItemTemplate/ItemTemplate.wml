<div class="controlsDemo__wrapper">
    <div class="controlsDemo__cell">
        <div class="controls-text-label">itemTemplate</div>
        <Controls.toggle:RadioGroup
                bind:selectedKey="_selectedKey"
                keyProperty="id"
                source="{{_source}}"
        >
            <ws:itemTemplate>
                <Controls-demo.toggle.RadioGroup.ItemTemplate.Template  item="{{item}}"
                                                                        readOnly="{{readOnly}}"
                                                                        selected="{{selected}}"/>
            </ws:itemTemplate>
        </Controls.toggle:RadioGroup>
    </div>
</div>


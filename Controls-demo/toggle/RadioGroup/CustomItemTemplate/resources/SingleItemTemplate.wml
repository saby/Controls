<span attr:class="controls-demo-radioGroup__itemWrapper">
    <ws:partial attr:class="controls-RadioItem__RadioCircle" template='Controls/toggle:switchCircleTemplate' item="{{item}}" readOnly="{{readOnly}}" selected="{{selected}}"/>
    <span attr:class="controls-text-secondary">{{item.title}}</span>
    <span attr:class="controls-RadioItem__caption_{{selected ? 'selected' : 'unselected'}}_{{readOnly ? 'disabled' : 'enabled'}}
                      controls-RadioItem__caption"> {{item.caption}}
    </span>
</span>

{
  "version": "0.2.0",
  "configurations": [
  {
    "name": "Launch Chrome",
    "request": "launch",
    "type": "pwa-chrome",
    "url": "http://localhost:8080",
    "webRoot": "${workspaceFolder}"
  },
    {
      "type": "node",
      "request": "launch",
      "name": "test",
      "protocol": "inspector",
      "program": "${workspaceFolder}/node_modules/wasaby-cli/cli.js",
      "args": [
        "--tasks=startTest",
        "--node",
        "--report=console",
        "--inspect",
        "--grep=''"
      ],
      "autoAttachChildProcesses": true,
      "console": "integratedTerminal"
    }
  ]
}
